<?xml version="1.0" encoding="utf-8"?>
<Report ScriptLanguage="CSharp" TextQuality="Regular">
  <Dictionary>
    <TableDataSource Name="AttendanceRecords">
      <Column Name="Date" DataType="System.DateTime"/>
      <Column Name="StudentName" DataType="System.String"/>
      <Column Name="Status" DataType="System.String"/>
      <Column Name="AttendanceRate" DataType="System.Decimal"/>
    </TableDataSource>
    <Parameter Name="GroupName" DataType="System.String"/>
    <Parameter Name="SubjectName" DataType="System.String"/>
    <Parameter Name="ProfessorName" DataType="System.String"/>
    <Parameter Name="DateRange" DataType="System.String"/>
    <Parameter Name="TotalSessions" DataType="System.Int32"/>
    <Parameter Name="TotalStudents" DataType="System.Int32"/>
    <Parameter Name="TotalPresent" DataType="System.Int32"/>
    <Parameter Name="TotalAbsent" DataType="System.Int32"/>
    <Parameter Name="AverageAttendance" DataType="System.Decimal"/>
  </Dictionary>
  <ReportPage Name="Page1" Watermark.Font="Arial, 60pt" Margins="20,20,20,20">
    <ReportTitleBand Name="ReportTitle1" Width="718.2" Height="132.3">
      <!-- Add actual title content -->
      <TextObject Name="Text1" Left="0" Top="0" Width="718.2" Height="47.25" 
                 Text="UniAttend" HorzAlign="Center" VertAlign="Center" 
                 Font="Arial, 28pt, style=Bold" TextFill.Color="Navy"/>
      <TextObject Name="Text2" Left="0" Top="47.25" Width="718.2" Height="37.8" 
                 Text="Group Attendance Report" HorzAlign="Center" VertAlign="Center" 
                 Font="Arial, 18pt"/>
      <TextObject Name="Text3" Left="9.45" Top="85.05" Width="699.3" Height="18.9" 
                 Text="Group: [GroupName]" Font="Arial, 12pt, style=Bold"/>
      <TextObject Name="Text4" Left="9.45" Top="103.95" Width="699.3" Height="18.9" 
                 Text="Period: [DateRange]" Font="Arial, 12pt"/>
    </ReportTitleBand>
    <DataBand Name="Data1" DataSource="AttendanceRecords">
      <!-- Add your data band content -->
      <TableObject Name="Table1" Left="9.45" Width="699.3" Height="28.35" Border.Lines="All">
        <TableColumn Width="150"/>
        <TableColumn Width="200"/>
        <TableColumn Width="150"/>
        <TableColumn Width="199.3"/>
        <TableRow>
          <TableCell Text="[AttendanceRecords.Date]" Format="Date" Format.Format="dd/MM/yyyy"/>
          <TableCell Text="[AttendanceRecords.StudentName]"/>
          <TableCell Text="[AttendanceRecords.Status]"/>
          <TableCell Text="[AttendanceRecords.AttendanceRate]%"/>
        </TableRow>
      </TableObject>
    </DataBand>
  </ReportPage>
</Report>